{
  "name": "@u1f992/gs-wasm",
  "version": "0.1.0",
  "description": "WASM build variant of ArtifexSoftware/ghostpdl 10.06.0.",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint --fix \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "build:wasm": "sh ./build-wasm.sh && shx cp ghostpdl/bin/gs.js src/gs.js && shx cp ghostpdl/bin/gs.wasm src/gs.wasm",
    "clean:wasm": "cd ghostpdl && emmake make clean && cd .. && shx rm src/gs.js src/gs.wasm",
    "build": "tsc && shx cp src/gs.js dist/gs.js && shx cp src/gs.wasm dist/gs.wasm",
    "clean": "shx rm -rf dist",
    "pretest": "npm run build",
    "test": "node --test \"dist/**/*.test.js\""
  },
  "keywords": [],
  "author": "Koutaro Mukai",
  "license": "AGPL-3.0-only",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/node": "^22.18.6",
    "eslint": "^9.36.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1"
  },
  "dependencies": {
    "web-worker": "^1.5.0"
  }
}
